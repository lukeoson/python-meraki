{
    "outbound_rules": [
      {
        "comment": "Allow traffic to AWS",
        "policy": "allow",
        "protocol": "tcp",
        "srcPort": "any",
        "srcCidr": "10.10.10.0/24",
        "destPort": "443",
        "destCidr": "10.100.0.0/16",
        "syslogEnabled": false
      }
    ],
    "inbound_rules": [
      {
        "comment": "Allow HTTP inbound between VLANs",
        "policy": "allow",
        "protocol": "tcp",
        "srcPort": "any",
        "srcCidr": "VLAN(10).*",
        "destPort": "80",
        "destCidr": "VLAN(20).*",
        "syslogEnabled": false
      }
    ]
  }